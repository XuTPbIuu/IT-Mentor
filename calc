def main(input: str) -> str:
    input = input.strip()
    parts = input.split()

    if len(parts) != 3:
        raise ValueError("throws Exception")

    a, operator, b = parts

    try:
        a = int(a)
        b = int(b)
    except ValueError:
        raise ValueError("throws Exception")

    if not (1 <= a <= 10) or not (1 <= b <= 10):
        raise ValueError("throws Exception")

    if operator == "+":
        result = a + b
    elif operator == "-":
        result = a - b
    elif operator == "*":
        result = a * b
    elif operator == "/":
        if b == 0:
            raise ZeroDivisionError("throws Exception")
        result = a // b  # Целочисленное деление
    else:
        raise ValueError("throws Exception")

    return str(result)


if __name__ == "__main__":
    try:
        user_input = input("Введите арифметическое выражение: ")
        output = main(user_input)
        print(output)
    except Exception:
        print("throws Exception")
